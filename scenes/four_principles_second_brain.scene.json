{
  "nodes": [
    {
      "id": "artifact/four_principles_second_brain",
      "type": "artifact",
      "label": "Four principles that separate successful AI builders (second brain case study)",
      "artifact_type": "video_summary",
      "source_url": "https://www.youtube.com/watch?v=_gPODg6br5w",
      "tags": ["second_brain", "ai_building", "knowledge_management", "2026"]
    },

    {
      "id": "principle/architecture_portable_tools_not",
      "type": "principle",
      "label": "Architecture is portable, tools are not"
    },
    {
      "id": "principle/principles_over_rules",
      "type": "principle",
      "label": "Principles-based guidance scales better than rules"
    },
    {
      "id": "principle/agent_builds_agent_maintains",
      "type": "principle",
      "label": "If the agent builds it, the agent can maintain it"
    },
    {
      "id": "principle/system_as_infrastructure",
      "type": "principle",
      "label": "Your system can be infrastructure, not just a tool"
    },

    {
      "id": "concept/capture_sort_store_retrieve_architecture",
      "type": "concept",
      "label": "Capture → sort/structure → store → intelligent retrieval"
    },
    {
