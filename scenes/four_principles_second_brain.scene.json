    },

    {
      "id": "pattern/notion_zapier_claude_stack",
      "type": "pattern",
      "label": "Notion + Zapier + Claude/ChatGPT second brain stack",
      "description": "Capture and route notes using Notion for storage, Zapier for automation, and Claude or ChatGPT as the intelligence layer."
    },
    {
      "id": "pattern/discord_obsidian_macwhisper_stack",
      "type": "pattern",
      "label": "Discord + Obsidian + MacWhisper capture stack",
      "description": "Discord as capture surface, MacWhisper for local transcription, and Obsidian for storage; slash commands trigger processing and filing by project."
    },
    {
      "id": "pattern/claude_computer_use_obsidian_typescript_vps",
      "type": "pattern",
      "label": "Claude computer-use + Obsidian + TypeScript agent on VPS",
      "description": "Claude’s computer-use plus a custom TypeScript agent on a VPS with Obsidian, guided by principle-level architectural best practices."
    },
    {
      "id": "pattern/meta_agent_multi_coding_assistants",
      "type": "pattern",
      "label": "Meta-agent framework orchestrating multiple coding assistants",
      "description": "A meta-agent coordinating Claude Code, Codex, Copilot, and Goose using a writer–critic loop, capable of setting up infra and generating UIs."
    },
    {
      "id": "pattern/postgres_vector_graph_infrastructure",
      "type": "pattern",
      "label": "Postgres + vector DB + graph DB as second brain infrastructure",
      "description": "Hybrid database system: Postgres for structured data, vector DB for semantic search, and Neo4j for relationships, exposed via API/SDK."
    },
    {
      "id": "pattern/notion_mobile_inbox_claude_scheduled",
      "type": "pattern",
      "label": "Notion mobile capture + scheduled Claude processing",
      "description": "Notion mobile export as capture mechanism, with Claude periodically processing an inbox and sorting into structured views."
    },
    {
      "id": "pattern/slack_yaml_claude_code_session",
      "type": "pattern",
      "label": "Slack capture + local YAML + Claude Code session-based processing",
      "description": "Slack as capture, local YAML files as storage, and Claude Code sessions that process inbox items, classify them, and write YAML on demand."
    },
    {
      "id": "pattern/zapier_slack_to_notion_classifier",
      "type": "pattern",
      "label": "Zapier Slack → Claude → Notion classification flow",
      "description": "Slack messages trigger a Zapier flow that calls Claude for JSON classification, cleans/parses results, routes to Notion DBs, logs inbox entries, and replies in Slack."
    },

